<!-- This is a sample XML file that is useful to test any parser that -->
<!-- uses the helloWorld.xsd schema defined for the Hello World sensor -->

<helloWorld xmlns="http://www.stdc.com/2001/QMS/helloWorld"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.stdc.com/2001/QMS/helloWorld helloWorld.xsd">

  <!-- The Hello World Sensor schema defines a request that contains
  two elements a firstname and a lastname.  -->
  
  <Request>
    <firstname>Foo</firstname>
    <lastname>Bar</lastname>
  </Request>
  
</helloWorld>           

<!-- This is a sample XML file that is useful to test any parser that -->
<!-- uses the perfmonProbes.xsd schema defined for the NT Performance -->
<!-- Monitor Probe -->


 <perfmonProbe xmlns="http://www.stdc.com/2001/QMS/Probes/perfmonProbes"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.stdc.com/2001/QMS/Probes/perfmonProbes
   perfmonProbes.xsd">
  
  <!-- The NT_Perf_Mon Continuous Probe accepts a request to start,
  pause, resume and stop it. If the contents of perfmonRequest is
  empty, the request is a modify request -->

  <perfmonRequest>
    <probeControl>
      <action>start</action>
    </probeControl>
  </perfmonRequest>
  
  </perfmonProbe>           

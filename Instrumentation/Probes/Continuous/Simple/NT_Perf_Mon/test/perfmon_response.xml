<!-- This is a sample XML file that is useful to test any parser that -->
<!-- uses the perfmonProbes.xsd schema defined for the NT Performance -->
<!-- Monitor Probe -->



  <perfmonProbe xmlns="http://www.stdc.com/2001/QMS/Probes/perfmonProbes"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.stdc.com/2001/QMS/Probes/perfmonProbes
    perfmonProbes.xsd">
  
  <!-- The NT_Perf_Mon Continuous Probe sends the resource value in its
  response. -->

  <perfmonData>
    <dataid>125</dataid>
    <resource>\System\% Total Processor Time</resource>
    <plotData>
      <numericData>92.5</numericData>
      <units>%</units>
    </plotData>
  </perfmonData>
  
  </perfmonProbe>           

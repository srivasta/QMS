<!-- This is a sample XML file that is useful to test any parser that -->
<!-- uses the commandProbe.xsd schema defined for the Command -->
<!-- Continuous Probe -->

  <commandProbe 
    xmlns="http://www.stdc.com/2001/QMS/Probes/Continuous/CommandProbe"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation=
    "http://www.stdc.com/2001/QMS/Probes/Continuous/CommandProbe 
    commandProbe.xsd">
    
    <!-- The Command Probe accepts a request to start and stop it. If
    the contents of probeRequest are empty, the request is a modify
    request. Sinceh -->
    
    <probeRequest>
      <probeCommand>
	<arg0>/bin/ls</arg0>
	<arg>-l</arg>
	<arg>/usr</arg>
      </probeCommand>
    </probeRequest>

  </commandProbe>           

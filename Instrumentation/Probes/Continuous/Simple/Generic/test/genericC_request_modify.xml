<!-- This is a sample XML file that is useful to test any parser that -->
<!-- uses the genericCProbe.xsd schema defined for the Generic -->
<!-- Continuous Probe -->

  <genericCProbe 
    xmlns="http://www.stdc.com/2001/QMS/Probes/Continuous/GenericProbe"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation=
    "http://www.stdc.com/2001/QMS/Probes/Continuous/GenericProbe 
    genericCProbe.xsd">
    
    <!-- The Generic Continuous Probe accepts a request to start and
    stop it. If the contents of probeRequest are empty, the request is
    a modify request -->
    
    <probeRequest>
    </probeRequest>

  </genericCProbe>           
